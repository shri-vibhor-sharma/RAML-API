#%RAML 1.0 DataType

type: object
displayName: Accounts Data Type
properties:
  id:
    type: integer
    description: unique identifier of accounts
  type:
    enum: [personal, business]

  firstName:
    type: string
    required: true
  lastName:
    type: string
    required: true
  address:
    type: string
  postal:
    type: string
  country:
    type: string
    minLength: 1
    maxLength: 999

  creationDate:
   type: datetime

  miles:
   type: integer
